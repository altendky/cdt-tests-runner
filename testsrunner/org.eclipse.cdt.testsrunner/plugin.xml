<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>
   <extension
         point="org.eclipse.debug.core.launchConfigurationTypes">
      <launchConfigurationType
            delegate="org.eclipse.cdt.testsrunner.internal.RunTestsLaunchDelegate"
            id="org.eclipse.cdt.testsrunner.launch.testsRunner"
            modes="debug,run"
            name="Run C/C++ Tests"
            public="true"
            sourceLocatorId="org.eclipse.cdt.debug.core.sourceLocator"
            sourcePathComputerId="org.eclipse.cdt.debug.core.sourcePathComputer">
      </launchConfigurationType>
   </extension>
   <extension
         point="org.eclipse.debug.ui.launchConfigurationTabGroups">
      <launchConfigurationTabGroup
            class="org.eclipse.cdt.testsrunner.internal.ui.launcher.TestsRunnerConfigurationTabGroup"
            description="Run C/C++ Tests"
            id="org.eclipse.cdt.testsrunner.launch.testsRunnerTabGroup"
            type="org.eclipse.cdt.testsrunner.launch.testsRunner">
      </launchConfigurationTabGroup>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <view
            category="org.eclipse.cdt.ui.views"
            class="org.eclipse.cdt.testsrunner.internal.ui.view.ResultsView"
            id="org.eclipse.cdt.testsrunner.resultsview"
            name="C++ Test Results"
            restorable="true">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <view
            category="org.eclipse.cdt.ui.views"
            class="org.eclipse.cdt.testsrunner.ViewPart1"
            id="org.eclipse.cdt.testsrunner.resultsview"
            name="C++ Test Results"
            restorable="true">
      </view>
   </extension>

   

   
   
</plugin>
